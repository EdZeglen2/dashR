<script language="javascript">

    var dashRApp = new dashR().newApp();
    
    dashRApp.load(function() {
       $("#imgLoading").hide();
    });
    
    var pageApp = new Vue({
        el: "#App",
        data: {
            app: dashRApp,
            datalocation: "",
            versionNo: dashR().versionInfo().number(),
            appName: "",
            onTab: "tab1",
            vsCode: "C:\\Users\\ezegl\\AppData\\Local\\Programs\\Microsoft VS Code\\code.exe",
            msHTA: "C:\\windows\\system32\\msHTA.exe",
            rootFolder: "",
        },
        methods: {
            init: function () {

                //console.log(this.app.folder);
                this.appName = this.app.shared("appName");
                this.rootFolder = (dashR("folder").parent(dashR("folder").parent(this.app.folder)));
                
                //console.log(this.app.folder);

                // var path1 = "c:\\ed\\";
                // var path2 = "\\fred";
                // var path3 = "ted\/";

                // var tt = dashR("path").combine([path1, path2, path3]);
                // console.log("path=" + tt);

                //alert(dashR('path').relative(this.app.folder, '..\\Dev\\elements\\file-selector'));
                
            },
            btnHere: function () {
                alert("here");
            },
            btnTesting: function () {

                

                var dateMod = dashR("date");

                console.log( dateMod.part.value( new Date() ));
                console.log( dateMod.part.compare( new Date(), new Date(1900, 1, 1), dateMod.cmpDirection.forward ));

                //jDash("file").save(this.page.data_folder + "\\test.txt", "This is and was a test");
                //jDash("system").open(this.page.data_folder + "\\test.txt");
            },
            fncURL: function (url) {
                dashR("system").open(url);
            },
            btn1: function () {
                alert("btn1");
            },
            btnNewApp: function () {
                var element = this;

                var appName = window.prompt("Enter a name for the new application.", "keep it simple");
                if (appName) {

                    // create the folder in Apps.

                    var newFolder = element.rootFolder + "\\Apps\\" + appName;
                    var basFolder = element.rootFolder + "\\Apps\\Empty";
                    var basFile = element.rootFolder + "\\Apps\\" + appName + "\\Empty.hta";
                    var newFile = appName + ".hta";

                    if (dashR("folder").exists(newFolder)) {
                        alert("Folder / Application of " + appName + " already exists!");
                    } else {

                        dashR("folder").create(newFolder);                        
                        dashR("folder").copy_to(newFolder, basFolder)
                        dashR("file").rename(basFile, newFile);
                        dashR("system").cmd(element.vsCode, newFolder);

                    }
                }

            },
            fncTab: function (id) {
                this.onTab = id;
                switch(this.onTab) {
                    case "tab1": {
                        this.fncLink("home");
                        break;
                    }                        
                    case "tab2": {
                        this.fncLink("ext_home")
                        break;
                    }
                    case "tab3": {
                        this.fncLink("files")
                        break;
                    }                             
                    case "tab4": {
                        this.fncLink("elements")
                        break;
                    }                         
                    case "tab5": {
                        this.fncLink("ext_file")
                        break;
                    }                         

                }
            },
            fncOpenFile: function (path) {
                //console.log(this.rootFolder + "\\Library\\Dev\\" + path);
                dashR("system").open(this.rootFolder + "\\Library\\Dev\\" + path);
            },
            openCode: function () {
                dashR("system").cmd (this.vsCode, this.rootFolder + "\\Library\\Dev\\elements\\icon-selector\\context_icons.js");
            },
            openHTA1: function () {
                dashR("system").cmd (this.msHTA, this.rootFolder + "\\Apps\\IconSelector\\IconSelector.hta");
            },
            openCode2: function () {
                dashR("system").cmd (this.vsCode, this.rootFolder + "\\Library\\Dev\\elements\\file-select\\file-selector.js");
            },
            openHTA2: function () {
                dashR("system").cmd (this.msHTA, this.rootFolder + "\\Apps\\Basic\\Basic.hta");
            },            
            fncLink: function (id) {
                this.app.ui.show(id);
            },
            fncOpen: function (url) {
                dashR("system").open(url);
            }
        }
